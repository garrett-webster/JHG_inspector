{
  "games": {
    "id": "INTEGER PRIMARY KEY AUTOINCREMENT",
    "gamesetId": "INTEGER",
    "code": "TEXT",
    "numPlayers": "INTEGER",
    "numObservers": "INTEGER",
    "status": "TEXT",
    "creatorId": "TEXT",
    "startDateTime": "TIMESTAMP",
    "name": "TEXT",

    "lengthOfRound": "INTEGER",
    "nameSet": "TEXT",
    "chatType": "TEXT",
    "messageType": "TEXT",
    "advancedGameSetup": "TEXT",

    "gameEndLow": "INTEGER",
    "gameEndHigh": "INTEGER",
    "gameEndType": "TEXT",

    "alpha": "REAL",
    "beta": "REAL",
    "cGive": "REAL",
    "cKeep": "REAL",
    "cSteal": "REAL",
    "povertyLine": "REAL",

    "govInitialPopularity": "INTEGER",
    "govInitialPopularityType": "TEXT",
    "govRandomPopularities": "BOOLEAN",
    "govRandomPopHigh": "INTEGER",
    "govRandomPopLow": "INTEGER",
    "govSendVotesImmediately": "BOOLEAN",

    "labelsEnabled": "BOOLEAN",

    "duration": "INTEGER",
    "runtimeType": "TEXT",

    "FOREIGN_KEYS": [
      {
        "column": "gamesetId",
        "references": {
          "table": "gamesets",
          "column": "id"
        }
      }
    ]
  },

  "gamesets": {
    "id": "INTEGER PRIMARY KEY AUTOINCREMENT",
    "name": "TEXT"
  },

  "players": {
    "id": "INTEGER PRIMARY KEY AUTOINCREMENT",
    "gameId": "INTEGER",
    "name": "TEXT",
    "experience": "TEXT",
    "permissionLevel": "TEXT",
    "color": "TEXT",
    "hue": "TEXT",
    "avatar": "TEXT",
    "icon": "TEXT",

    "FOREIGN_KEYS": [
      {
        "column": "gameId",
        "references": {
          "table": "games",
          "column": "id"
        }
      }
    ]
  }
}
